{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "declaration": true
  },
  "exclude": ["node_modules", "dist"]
}
